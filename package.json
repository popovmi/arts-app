    "license": "MIT",
    "scripts": {
        "test": "nx test",
        "gen:gql:all": "npx gqlg --schemaFilePath ./apps/api/src/schema.gql --destDirPath ./.temp",
        "gen:gql:ui": "graphql-codegen",
        "dev:api": "NODE_ENV=development nx serve api",
        "dev:ui": "NODE_ENV=development nx serve ui",
        "dev:all": "NODE_ENV=development nx run-many --target=serve --projects=api,ui --parallel=2",
        "build:api": "nx run api:build-app",
        "build:ui": "nx run build ui",
        "build": "npm run build:api && npm run build:ui",
        "migration:show": "ts-node --project=apps/api/tsconfig.app.json --transpile-only -r tsconfig-paths/register  ./node_modules/typeorm/cli.js  migration:show -d apps/api/datasource.local.js",
        "migration:run": "ts-node --project=apps/api/tsconfig.app.json --transpile-only -r tsconfig-paths/register  ./node_modules/typeorm/cli.js  migration:run -d apps/api/datasource.local.js",
        "migration:revert": "ts-node --project=apps/api/tsconfig.app.json --transpile-only -r tsconfig-paths/register  ./node_modules/typeorm/cli.js  migration:revert -d apps/api/datasource.local.js",
        "migration:create": "typeorm migration:create -o"
    },
