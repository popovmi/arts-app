    "gen:gql:ui": "graphql-codegen",
    "dev:api": "NODE_ENV=development nx serve api",
    "dev:ui": "NODE_ENV=development nx serve ui",
    "dev:all": "NODE_ENV=development nx run-many --target=serve --projects=api,ui --parallel=2",
    "start": "node dist/main.js",
    "migration:generate": " ts-node --project=apps/api/tsconfig.app.json --transpile-only -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -f apps/api/ormconfig.local.js -o -p  -n",
    "migration:revert": "ts-node --project=apps/api/tsconfig.app.json --transpile-only -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -f apps/api/ormconfig.local.js",
